{
  "name": "simp-foreman",
  "version": "1.0.0",
  "author": "SIMP Team",
  "summary": "A puppet module to install and configure Foreman.",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-foreman",
  "project_page": "https://github.com/simp/pupmod-simp-foreman",
